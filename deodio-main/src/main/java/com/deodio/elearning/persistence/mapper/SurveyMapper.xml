  <!-- 根据问卷调查的id获取相同账户下的讲师 -->
    <select id="findShareSurveyTrainee" parameterType="java.util.Map" resultType="java.util.Map">  
		<!-- SELECT DISTINCT ts.id,tg.account_id,case when tu.user_name is null then
			case when tu.nick_name is null then
				concat(tu.first_name,'.',tu.last_name)
			else tu.nick_name end
		else tu.user_name end as user_name,tu.user_mail,tu.id as user_id
		FROM t_survey ts JOIN t_group tg on ts.account_id=tg.account_id
		JOIN t_group_role_user_rel tgrur 
		on tgrur.group_id = tg.id JOIN t_user tu on 
		tgrur.user_id=tu.id 
		WHERE tgrur.group_role_id ='10001' 
		and ts.id=#{surveyId,jdbcType=VARCHAR} 
    	and ts.create_id !=tgrur.user_id
     <if test="userName != null">
     and (tu.user_name like CONCAT('%',#{userName},'%' )
     or tu.nick_name like CONCAT('%',#{userName},'%' )
     or tu.first_name like CONCAT('%',#{userName},'%' )
     or tu.last_name like CONCAT('%',#{userName},'%' )
     or tu.user_mail like CONCAT('%',#{userName},'%' )
     )
     </if> -->
     		SELECT DISTINCT tuar.account_id,case when tu.user_name is null then
			case when tu.nick_name is null then
				concat(tu.first_name,'.',tu.last_name)
			else tu.nick_name end
		else tu.user_name end as user_name,tu.user_mail,tu.id as user_id
		FROM t_user tu LEFT JOIN t_user_account_rel tuar ON tuar.user_id = tu.id
		LEFT JOIN t_group_role_user_rel tgrur ON tgrur.user_id = tu.id
		LEFT JOIN t_group_role tgr ON tgr.id = tgrur.group_role_id
		WHERE tgr.name = 'Content Creator' 
		AND tuar.account_id = #{accountId, jdbcType=VARCHAR}
		AND tu.id != #{createId, jdbcType=VARCHAR}
		 <if test="userName != null">
     and (tu.user_name like CONCAT('%',#{userName},'%' )
     or tu.nick_name like CONCAT('%',#{userName},'%' )
     or tu.first_name like CONCAT('%',#{userName},'%' )
     or tu.last_name like CONCAT('%',#{userName},'%' )
     or tu.user_mail like CONCAT('%',#{userName},'%' )
     )
