<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		creationComplete="init()"
		alpha.disabled=".5" xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	<fx:Script>
		<![CDATA[
			protected function init():void{
				
			}
		]]>
	</fx:Script>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	
	<s:Rect id="rect" left="0" right="0" top="0" bottom="0">
		<s:fill>
			<s:SolidColor alpha="1" color="#526c8d"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke alpha="1" color="#526c8d" weight="1"/>
		</s:stroke>
	</s:Rect>
	
	
	<mx:Text id="lableElement" horizontalCenter="0" text="{hostComponent.label}"
			 fontSize="14" verticalCenter="1" fontFamily="yahei" fontWeight="bold" color="#d0d0d0"/>
</s:Skin>